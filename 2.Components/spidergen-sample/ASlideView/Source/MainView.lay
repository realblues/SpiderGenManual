<div data-base="AView" data-class="MainView" class="AView-Style" style="left: 0px; top: 0px; width: 100%; height: 100%; background-color: rgb(239, 239, 239); position: absolute;" data-class-map="{&quot;afc&quot;:[&quot;AView&quot;,&quot;ASlideView&quot;,&quot;AButton&quot;]}">
    <div data-base="ASlideView" data-class="ASlideView" class="ASlideView-Style" style="height: 300px; z-index: 0; white-space: nowrap; position: absolute; left: 60px; top: 20px; width: calc((100% - 60px) - 60px); border-width: 1px;" data-direction="horizontal" id="MainView--SlideView" data-sgap-width="1"></div>
    <button data-base="AButton" data-class="AButton" class="AButton-Style" style="width: 50px; height: 300px; position: absolute; top: 20px; background-color: rgb(255, 255, 255); border-width: 1px; right: 10px;" data-listener-click="MainView:rightBtnClick" id="MainView--leftBtn">&gt;</button>
    <button data-base="AButton" data-class="AButton" class="AButton-Style" style="width: 50px; height: 300px; position: absolute; left: 10px; top: 20px; background-color: rgba(0, 0, 0, 0); border-width: 1px;" data-listener-click="MainView:leftBtnClick" id="MainView--rightBtn">&lt;</button>
</div>