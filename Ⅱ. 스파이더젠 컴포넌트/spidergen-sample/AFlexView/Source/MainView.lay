<div data-base="AView" data-class="MainView" class="AView-Style" style="left: 0px; top: 0px; width: 100%; height: 100%; background-color: rgb(239, 239, 239); position: absolute;" data-class-map="{&quot;afc&quot;:[&quot;AView&quot;,&quot;AFlexView&quot;,&quot;ALabel&quot;,&quot;AButton&quot;]}">
    <div data-base="AFlexView" data-class="AFlexView" class="AFlexView-Style" style="width: calc((100% - 20px) - 20px); height: calc((100% - 60px) - 30px); position: absolute; left: 20px; top: 60px;" id="MainView--flexView" data-sgap-width="1" data-sgap-height="1"></div>
    <label data-base="ALabel" data-class="ALabel" class="ALabel-Style" style="width: auto; height: auto; position: absolute; left: 20px; top: 30px; font-size: 15px;" id="MainView--lblViewCnt">views :</label>
    <button data-base="AButton" data-class="AButton" class="AButton-Style" style="width: 80px; height: 40px; position: absolute; top: 10px; right: 20px;" id="MainView--btnAddView" data-listener-click="MainView:onBtnAddViewClick">Add</button>
</div>